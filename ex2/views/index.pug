doctype html
html
  head
    title #{title}
  body
    h1 #{title}
    table
      thead
        tr
          th ID
          th Designação
          th Data
          th # Produtos
      tbody
        each lista in listas
          tr
            td
              a(href=`/listas/${lista._id}`) #{lista._id}
            td #{lista.designacao}
            td #{lista.data}
            td #{lista.produtos.length}
